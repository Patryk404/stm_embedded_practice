#include <stdint.h>

void generate_interrupt(void){

}

void change_access_level_unpriv(void){
	uint32_t control_reg=0;
	__asm volatile("MRS %0,CONTROL":"=r"(control_reg)::); // load value of control register

	control_reg |= 1; // setting 0 bit to 1

	// it can be done just with assembly like that
	/*
	 * __asm volatile("MRS R0,CONTROL");
	 * __asm volatile("ORR R0,RO,#0x01");
	 * __asm volatile("MSR CONTROL,R0");
	 *
	 * */

	__asm volatile("MSR CONTROL,%0"::"r"(control_reg):); // moving value to control register

}

int main(void)
{
    /* Loop forever */

	change_access_level_unpriv();

	for(;;);
}
