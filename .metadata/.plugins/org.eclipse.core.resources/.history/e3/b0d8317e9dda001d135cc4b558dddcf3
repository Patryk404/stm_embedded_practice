#include <stdint.h>

int main(void)
{
    /* Loop forever */
	__asm volatile("LDR R0,=#0xff");
	__asm volatile("LDR R1,=#0x20000200");
	__asm volatile("STR R1,[R0]");
	__asm volatile("LDR R2,=#0x80");
	__asm volatile("AND R0,R2");
	__asm volatile("STR R1,[R0]");


	for(;;);
}
